<div class="panel">
  <h3>Response Details</h3>
  <div class="panel_contents">
    <div class="attributes_table response" id="attributes_table_response_1124">
      <table border="0" cellspacing="0" cellpadding="0">
        <tbody><tr>
          <th>Id</th>
          <td>1124</td>
        </tr>
        <tr>
          <th>Responder</th>
          <td><%= @response.responder.phone_number %></td>
        </tr>
        <tr>
          <th>Survey</th>
          <td><%= link_to @response.segment.survey.name, admin_survey_path(@response.segment.survey) %></td>
        </tr>
        <tr>
          <th>Segment</th>
          <td><%= @response.segment.name %></td>
        </tr>
        <tr>
          <th>Created At</th>
          <td><%= @response.created_at.to_s %></td>
        </tr>
        <tr>
          <th>Updated At</th>
          <td><%= @response.updated_at.to_s %></td>
        </tr>
        <tr>
          <th>Complete</th>
          <td><%= @response.complete %></td>
        </tr>
      </tbody></table>
    </div>
  </div>
</div>

<div class="panel">
  <h3>Answers</h3>
  <div class="panel_contents">
    <dl>
      <% @response.survey.questions.each do |question| %>
      <dt><%= question.text %></td>
      <dd><%= @response.answer_for_question(question).try(:text) %></td>
      <% end %>
    </dl>
  </div>
</div>
